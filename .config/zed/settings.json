{
  "assistant": {
    "version": "2",
    "default_model": {
      "provider": "ollama",
      "model": "llama3.1:latest"
    }
  },
  "features": {
    "inline_completion_provider": "supermaven"
  },
  "theme": "Catppuccin Mocha",
  "buffer_font_size": 18,
  "buffer_font_family": "JetBrainsMonoNL Nerd Font",
  "languages": {
    "Markdown": {
      "format_on_save": "off"
    },
    "TypeScript": {
      "language_servers": ["!typescript-language-server", "vtsls", "..."]
    },
    "TSX": {
      "language_servers": ["!typescript-language-server", "vtsls", "..."]
    },
    "JavaScript": {
      "language_servers": ["!typescript-language-server", "vtsls", "..."]
    }
  }
}
